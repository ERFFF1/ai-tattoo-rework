# AI Tattoo Rework v1.4 - Environment Variables
# Copy this file to .env.local and fill in your actual values

# AI Providers
LEONARDO_API_KEY=your_leonardo_api_key_here
STABILITY_API_KEY=your_stability_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-flash

# AI Configuration
IMAGE_PROVIDER_ORDER=leonardo,stability
STRENGTH_DEFAULT=0.4
OUTPUT_MAX_PX=4096

# Storage
STORAGE_DRIVER=blob

# Payments (Stripe)
STRIPE_SECRET=sk_live_your_stripe_secret_key
STRIPE_PRICE_SINGLE=price_your_single_export_price_id
STRIPE_PRICE_MONTHLY=price_your_monthly_subscription_price_id
STRIPE_PRICE_STUDIO=price_your_studio_plan_price_id
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# System
NEXT_PUBLIC_URL=https://your-domain.com

# Rate Limiting (Upstash Redis)
UPSTASH_REDIS_REST_URL=https://your-redis-url.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_redis_token

# Quality Gate (Optional)
VISION_QGATE=off
OPENAI_API_KEY=your_openai_api_key_here

# Database
DATABASE_URL=postgresql://user:password@host:port/database
